xkb_symbols "thinkpad" {
  key <CAPS> { [ Control_L ] };
  key <RTRN> { [ Control_R ] };
  key <LWIN> { [ Alt_L ] };
  key <PRSC> { [ Alt_R ] };
  //key <LALT> { [ Super_L ] };
  //key <RALT> { [ Super_R ] };
  key <RCTL> { [ Return ] };

  modifier_map Control { Control_L, Control_R, <CAPS> };
  modifier_map Mod1 { <LWIN>, <PRSC> };
  //modifier_map Mod4 { Super_L, Super_R };
};
